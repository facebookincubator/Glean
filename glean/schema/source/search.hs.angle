schema search.hs.9 {
  import code.hs
  import hs.2

  predicate SearchByName:
    {
      name: string,
      entity: code.hs.Entity,
    } { Name, { name = Entity } } where Entity.occ.name = Name;

  predicate SearchByLowerCaseName:
    {
      name: string,
      entity: code.hs.Entity,
    } { Name, { name = Entity } } where
      hs.OccNameLowerCase { Name, Occ };
      Entity.occ = Occ;

}
